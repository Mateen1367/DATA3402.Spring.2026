    "\n",
    "class Rectangle(Shape):\n",
    "    def __init__(self,l,w,x,y):\n",
    "        self.l,self.w,self.x,self.y = l,w,x,y\n",
    "\n",
    "    def area(self): return self.l*self.w\n",
    "    def perimeter(self): return 2*(self.l+self.w)\n",
    "\n",
    "    def points(self):\n",
    "        return [(self.x,self.y),\n",
    "                (self.x+self.l,self.y),\n",
    "                (self.x+self.l,self.y+self.w),\n",
    "                (self.x,self.y+self.w)]\n",
    "\n",
    "    def contains(self,x,y):\n",
    "        return (self.x <= x <= self.x+self.l and\n",
    "                self.y <= y <= self.y+self.w)\n",
    "\n",
    "\n",
    "class Circle(Shape):\n",
    "    def __init__(self,r,x,y):\n",
    "        self.r,self.x,self.y = r,x,y\n",
    "\n",
    "    def area(self): return math.pi*self.r**2\n",
    "    def perimeter(self): return 2*math.pi*self.r\n",
    "\n",
    "    def points(self):\n",
    "        return [(self.x+self.r*math.cos(2*math.pi*i/16),\n",
    "                 self.y+self.r*math.sin(2*math.pi*i/16)) for i in range(16)]\n",
    "\n",
    "    def contains(self,x,y):\n",
    "        return (x-self.x)**2 + (y-self.y)**2 <= self.r**2\n",
    "\n",
    "\n",
