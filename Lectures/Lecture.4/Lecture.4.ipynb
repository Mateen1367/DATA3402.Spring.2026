    "def pick_move(board,player,depth=5,func=max):\n",
    "    moves,scores=generate_moves(board,player,player,depth)\n",
    "    result=list(map(tree_search,scores))\n",
    "    move_index=result.index(func(result))\n",
    "    return moves[move_index][0]"
